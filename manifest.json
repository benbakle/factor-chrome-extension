{
    "manifest_version": 2,
    "name": "Factor Dark",
    "description" : "Factor Dark : Dark mode for Factor project tracking system",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "factor-icon.png"
    },
    "icons": {
        "16": "factor-icon.png",
        "48": "factor-icon.png",
        "128": "factor-icon.png"
      },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}